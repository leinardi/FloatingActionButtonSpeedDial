<resources>
    <declare-styleable name="FabWithLabelView">
        <attr name="fab_label" format="string" />
        <attr name="fab_color" format="color" />
        <attr name="fab_drawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="FabMenuView">
        <attr name="options_menu" format="reference" />
        <attr name="options_color" format="color" />
        <attr name="open_src" format="reference" />
        <attr name="close_src" format="reference" />
        <attr name="color" format="color" />
    </declare-styleable>

    <declare-styleable name="FabOverlayLayout">
        <attr name="background_color" format="color" />
        <attr name="clickable_overlay" format="boolean" />
    </declare-styleable>
</resources>
